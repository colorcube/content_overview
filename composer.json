{
    "name": "colorcube/content-overview",
    "type": "typo3-cms-extension",
    "description": "TYPO3 backend module which gives and overview of the the content elements in a page tree.",
    "keywords": [
        "TYPO3",
        "extension",
        "content_overview"
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://bitbucket.org/colorcube/content_overview",
    "support": {
        "issues": "https://bitbucket.org/colorcube/content_overview/issues"
    },
    "authors": [
        {
            "name": "Ren√© Fritz",
            "email": "r.fritz@colorcube.de",
            "role": "Developer",
            "homepage": "http://www.colorcube.de"
        }
    ],
    "require": {
        "typo3/cms-core": "^7.6 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Colorcube\\ContentOverview\\": "Classes"
        }
    },
    "replace": {
        "content_overview": "self.version",
        "typo3-ter/content-overview": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "content_overview"
        }
    }
}
